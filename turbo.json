{
  "ui": "stream",
  "globalPassThroughEnv": [
    "RPC_URL_MAINNET",
    "RPC_URL_OPTIMISM",
    "RPC_URL_BASE"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "cache/**", "src/generated/**"]
    },
    "build:zk": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "artifacts-zk/**", "cache-zk/**"]
    },
    "build:tron": {
      "dependsOn": ["build"],
      "outputs": ["cache-tron/**", "artifacts-tron/**"]
    },
    "test:ci": {
      "dependsOn": ["build", "^test:ci"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "format": {},
    "lint": {},
    "version:update": {
      "cache": false
    },
    "coverage": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**", "fixtures/**"]
    },
    "bundle": {
      "dependsOn": ["build"],
      "outputs": ["bundle/**"]
    }
  }
}
