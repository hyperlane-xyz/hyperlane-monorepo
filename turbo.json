{
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "cache/**", "src/generated/**", ".next/**", "!.next/cache/**"]
    },
    "build:zk": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "artifacts-zk/**", "cache-zk/**"]
    },
    "test:ci": {
      "dependsOn": ["build", "^test:ci"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "prettier": {},
    "lint": {},
    "version:update": {
      "cache": false
    },
    "coverage": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**", "fixtures/**"]
    },
    "bundle": {
      "dependsOn": ["^build"],
      "outputs": ["*-bundle/**"]
    }
  }
}
