{
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "cache/**", "src/generated/**"]
    },
    "build:zk": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "artifacts-zk/**", "cache-zk/**"]
    },
    "build:tron": {
      "dependsOn": ["build"],
      "outputs": [
        "cache-tron/**",
        "../typescript/tron-sdk/src/abi/**",
        "../typescript/tron-sdk/src/typechain/**"
      ]
    },
    "test:ci": {
      "dependsOn": ["build", "^test:ci"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "format": {},
    "lint": {},
    "version:update": {
      "cache": false
    },
    "coverage": {
      "dependsOn": ["build"],
      "outputs": ["coverage/**", "fixtures/**"]
    },
    "bundle": {
      "dependsOn": ["build"],
      "outputs": ["bundle/**"]
    }
  }
}
