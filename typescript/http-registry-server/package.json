    "express": "^5.1.0",
    "pino": "^8.19.0",
    "zod": "^3.21.2"
  },
  "devDependencies": {
    "@hyperlane-xyz/tsconfig": "workspace:^",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^8",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.5",
    "@types/sinon": "^17.0.1",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.32",
    "chai": "^4.5.0",
    "chai-as-promised": "^8.0.1",
    "eslint": "^9.15.0",
    "mocha": "^11.5.0",
    "prettier": "^3.5.3",
    "sinon": "^13.0.2",
    "supertest": "^6.3.3",
    "tsx": "^4.19.1",
    "typescript": "5.3.3",
    "yargs": "^17.7.2"
  }
