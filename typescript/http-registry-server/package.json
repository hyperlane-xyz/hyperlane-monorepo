{
  "name": "@hyperlane-xyz/http-registry-server",
  "version": "1.0.0",
  "private": true,
  "description": "An HTTP server for the Hyperlane registry",
  "license": "Apache 2.0",
  "homepage": "https://www.hyperlane.xyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyperlane-xyz/hyperlane-monorepo"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write ./src ./test",
    "test": "yarn test:unit",
    "test:ci": "yarn test",
    "test:unit": "mocha --config .mocharc.json './test/**/*.ts' --exit"
  },
  "dependencies": {
    "@hyperlane-xyz/registry": "17.8.2",
    "@hyperlane-xyz/sdk": "14.2.0",
