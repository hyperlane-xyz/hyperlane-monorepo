{
  "name": "@hyperlane-xyz/radix-sdk",
  "version": "20.1.0",
  "description": "Hyperlane TypeScript SDK for the Radix Hyperlane SDK module",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "docker-compose.yml"
  ],
  "homepage": "https://www.hyperlane.xyz",
  "repository": "https://github.com/hyperlane-xyz/hyperlane-monorepo",
  "keywords": [
    "hyperlane",
    "radix",
    "blockchain",
    "sdk"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "format": "prettier --write .",
    "lint": "eslint -c ./eslint.config.mjs",
    "prettier": "prettier --write ./src",
    "clean": "rm -rf ./dist ./cache",
    "dev": "tsc --watch",
    "test": "echo \"no tests in radix-sdk\"",
    "test:unit": "mocha --config .mocharc.json",
    "test:ci": "pnpm test:unit",
    "test:e2e": "mocha --config .mocharc.e2e.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@hyperlane-xyz/eslint-config": "workspace:^",
    "@hyperlane-xyz/tsconfig": "workspace:^",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.1.6",
    "@typescript-eslint/parser": "^8.1.6",
    "chai": "^4.5.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "mocha": "^11.5.0",
    "mocha-steps": "^1.3.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "testcontainers": "11.7.0",
    "tsx": "^4.19.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.37.0"
  },
  "dependencies": {
    "@hyperlane-xyz/provider-sdk": "workspace:*",
    "@hyperlane-xyz/utils": "workspace:*",
    "@radixdlt/babylon-core-api-sdk": "^1.3.0",
    "@radixdlt/babylon-gateway-api-sdk": "^1.10.1",
    "@radixdlt/radix-engine-toolkit": "^1.0.5",
    "bignumber.js": "^9.1.1",
    "decimal.js": "^10.4.3",
    "ethers": "^5.8.0"
  }
}
