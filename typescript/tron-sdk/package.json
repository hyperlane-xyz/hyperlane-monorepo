{
  "name": "@hyperlane-xyz/tron-sdk",
  "version": "23.0.0",
  "description": "Hyperlane TypeScript SDK for Tron",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "default": "./dist/testing/index.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "/dist",
    "local-node"
  ],
  "homepage": "https://www.hyperlane.xyz",
  "repository": "https://github.com/hyperlane-xyz/hyperlane-monorepo",
  "keywords": [
    "hyperlane",
    "tron",
    "blockchain",
    "sdk"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "pnpm build:sol && rm -rf ./dist && tsc",
    "build:sol": "pnpm -C ../../solidity build:tron",
    "format": "prettier --write .",
    "lint": "eslint -c ./eslint.config.mjs",
    "prettier": "prettier --write ./src",
    "clean": "rm -rf ./dist ./cache ./src/typechain ./coverage",
    "dev": "tsc --watch",
    "test": "mocha --config .mocharc.json './src/**/*.test.ts'",
    "test:e2e": "pnpm test",
    "coverage": "c8 pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "catalog:",
    "@hyperlane-xyz/eslint-config": "workspace:^",
    "@hyperlane-xyz/tsconfig": "workspace:^",
    "@types/chai": "catalog:",
    "@types/mocha": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "c8": "catalog:",
    "chai": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-import-resolver-typescript": "catalog:",
    "eslint-plugin-import": "catalog:",
    "mocha": "catalog:",
    "prettier": "catalog:",
    "testcontainers": "catalog:",
    "ts-node": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "dependencies": {
    "@ethersproject/abi": "*",
    "@ethersproject/providers": "*",
    "@hyperlane-xyz/core": "workspace:*",
    "@hyperlane-xyz/provider-sdk": "workspace:*",
    "@hyperlane-xyz/utils": "workspace:*",
    "ethers": "catalog:",
    "tronweb": "catalog:"
  }
}
